meContext from './ThemeContext';\n\nconst SearchParams = () => {\n  const [location, setLocation] = useState(\"Seattle, WA\");\n  const [breeds, setBreeds] = useState([]);\n  const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\n  const [breed, BreedDropdown, setBreed] = useDropdown(\"Breed\", \"\", breeds);\n  const [pets, setPets] = useState([]); // when you first request a pet, there isnt going to be anything in there\n  const [theme, setTheme] = useContext(ThemeContext);\n  async function requestPets() {\n    const { animals } = await pet.animals({\n      location,\n      breed,\n      type: animal,\n    });\n\n    setPets(animals || []);\n  }\n\n  useEffect(() => {\n    setBreeds([]);\n    setBreed(\"\");\n\n    pet.breeds(animal).then(({ breeds: apiBreeds }) => {\n      const breedStrings = apiBreeds.map(({ name }) => name);\n      setBreeds(breedStrings); // from the setBreeds function on line 7\n    }, console.error);\n  }, [animal, setBreeds, setBreed]);\n  return (\n    <div className=\"search-params\">\n      <form\n        onSubmit={(e) => {\n          e.preventDefault(); //doesnt automatically\n          requestPets();\n        }}\n      >\n        <label htmlFor=\"location\">\n          Location\n          <input\n            id=\"location\"\n            value={location}\n            placeholder=\"location\"\n            onChange={(e) => setLocation(e.target.value)}\n          />\n        </label>\n        <AnimalDropdown />\n        <BreedDropdown />\n        <label htmlFor=\"theme\"> \n        Theme \n        <select \n        value = {theme}\n        onChange = {e => setTheme(e.target.value)}\n        onBlur = {e => setTheme(e.target.value)} \n          >\n          <option value =\"peru\"> Peru </option>\n          <option value =\"red\"> Red </option>\n          <option value =\"darkblue\"> Dark Blue </option>\n          \n\n          </select>       \n        </label>\n\n        <button style={{ backgroundColor : theme }}>Submit</button>\n      </form>\n      <Results pets={pets} />\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"a60961dbeb879331532cb3483f9ca1ee","cacheData":{"env":{}}}